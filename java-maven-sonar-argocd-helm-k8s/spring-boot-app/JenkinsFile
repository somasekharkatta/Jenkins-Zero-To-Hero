pipeline {
  agent  any
  stages {

    stage('Checkout') {
      steps {
        sh 'echo passed'
        //git branch: 'main', url: 'https://github.com/iam-veeramalla/Jenkins-Zero-To-Hero.git'
      }
     }
   

        stage('Build') {
            steps {
                sh 'mvn clean install'
            }
         }

    }
}
